{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "google",
    "plugin:vue/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "vue/require-prop-types": "off",
    "vue/no-v-html": "off",
    "max-len": "off",
    "require-jsdoc": "off",
    "no-invalid-this": "off"
  }
}
